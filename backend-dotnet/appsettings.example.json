{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=armwrestling;Username=postgres;Password=your_password_here"
  },
  "Supabase": {
    "Url": "https://your-project.supabase.co",
    "AnonKey": "your_supabase_anon_key_here",
    "ServiceRoleKey": "your_supabase_service_role_key_here"
  },
  "Jwt": {
    "Key": "your-super-secret-jwt-key-at-least-32-characters-long",
    "Issuer": "ArmWrestlingApi",
    "Audience": "ArmWrestlingApp",
    "ExpiresInDays": 30
  }
}
